<%- include ('../layouts/header') %>
<%- include ('../layouts/header_options') %>
<h2><%=title%></h2>
<form class="form" name="myForm" action="/usuario/edit" method="post" enctype="application/x-www-form-urlencoded">
    <div class="form-label-group" hidden>
      <input type="number" class="form-control" id="id" name="id" required="" placeholder="id" value="<%= data.id %>">
    </div>
    <div class="form-label-group">
        <input type="password" class="form-control" id="clave" name="clave" required="" placeholder="Clave" value="<%= data.clave %>">
    </div>
    <button type="submit" class="btn btn-primary">Modificar</button>
  </form>

  <%- include ('../layouts/footer') %>
  